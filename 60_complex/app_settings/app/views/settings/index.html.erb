<h1>Settings</h1>
 
<table>
  <tr>
    <th>Key</th>
    <th>URL</th>
    <th></th>
    <th></th>
  </tr>
 
  <% @settings.each do |setting| %>
    <tr>
      <td><%= setting.key %></td>
      <td><%= setting.url %></td>
      <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
      <td><%= link_to 'Delete', setting_path(setting), 
                    method: :delete, data: { confirm: 'Are you sure to delete the setting?' } %></td>
      </tr>
  <% end %>
</table>
<%= link_to 'New Setting', new_setting_path %></td>